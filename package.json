{
  "name": "gps-tools-frontend",
  "private": true,
  "version": "0.16.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "rm dist/mockServiceWorker.js && rm -rf dist/mocks",
    "test": "vitest",
    "test:e2e": "npx playwright test",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "check": "eslint . --report-unused-disable-directives --max-warnings 0 && prettier --check ./src && vitest --watch=false",
    "check:fix": "eslint . --fix && prettier --write ./src && vitest --watch=false",
    "preview": "vite preview",
    "upgrade": "ncu -u",
    "upgrade:patches": "ncu -u --target patch",
    "upgrade:minor": "ncu -u --target minor"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.5",
    "@hello-pangea/dnd": "^18.0.1",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-query-devtools": "^5.67.2",
    "@tanstack/react-router": "^1.112.18",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.16",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.8.2",
    "dompurify": "^3.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "gpxparser": "^3.0.8",
    "hamburger-react": "^2.5.2",
    "leaflet": "^1.9.4",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.3.0",
    "react-use": "^17.6.0",
    "sanitize-html": "^2.14.0",
    "uuid": "^11.1.0",
    "vite-plugin-package-version": "^1.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.51.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/postcss": "^4.0.12",
    "@tanstack/eslint-plugin-query": "^5.67.2",
    "@tanstack/router-devtools": "^1.112.18",
    "@tanstack/router-vite-plugin": "^1.112.19",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.13.9",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sanitize-html": "^2.13.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "msw": "^2.7.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.34.9",
    "@swc/core-linux-x64-gnu": "^1.11.8",
    "@tailwindcss/oxide-linux-x64-gnu": "^4.0.12",
    "lightningcss-linux-x64-gnu": "^1.29.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
